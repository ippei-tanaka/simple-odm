{
  "name": "simple-odm",
  "version": "0.4.0",
  "description": "a simple object document mapper",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:src",
    "build:clean": "./node_modules/.bin/rimraf lib",
    "build:src": " ./node_modules/.bin/babel src --out-dir lib",
    "test": "./node_modules/.bin/mocha $(find test -name '*.test.js') --compilers js:babel-register --require babel-polyfill",
    "test:unit": "./node_modules/.bin/mocha $(find test/unit -name '*.test.js') --compilers js:babel-register --require babel-polyfill",
    "test:integration": "./node_modules/.bin/mocha $(find test/integration -name '*.test.js') --compilers js:babel-register --require babel-polyfill"
  },
  "author": {
    "name": "Ippei Tanaka",
    "email": "tanaka.ippei.jp@gmail.com",
    "url": "http://www.ipe.club/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ippei-tanaka/simple-odm.git"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "co": "^4.6.0",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.2",
    "uuid-validate": "0.0.2",
    "validator": "^5.4.0"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "deepcopy": "^0.6.3",
    "immutable": "^3.8.1",
    "mongodb": "^2.1.18",
    "pluralize": "^3.0.0",
    "uuid-validate": "0.0.2"
  }
}
